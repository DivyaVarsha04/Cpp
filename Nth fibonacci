#include<bits/stdc++.h>
using namespace std;

int fib(int n)
{
        /*
        *  Write your code here. 
         *  Read input as specified in the question.
         *  Print output as specified in the question.
        */
        if(n==0) return 0;
        else if(n==1 || n==2) return 1;
        else 
        return fib(n-1)+fib(n-2);

}
int main()
{
        int n;
        cin>>n;
      cout<<fib(n)<<endl;
       
}
